<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no" />
    <title>Nebenan newsletter</title>

    <link rel="stylesheet" href="src/styles/defaults/overrides.css">
    <link rel="stylesheet" href="src/styles/defaults/copy.css">
    <link rel="stylesheet" href="src/styles/defaults/emoji.css">
    <link rel="stylesheet" href="src/styles/defaults/grid.css">
    <link rel="stylesheet" href="src/styles/defaults/button.css">

    <link rel="stylesheet" href="src/styles/partials/card.css">
    <link rel="stylesheet" href="src/styles/partials/layout.css">
    <link rel="stylesheet" href="src/styles/partials/header.css">
    <link rel="stylesheet" href="src/styles/partials/separator.css">
    <link rel="stylesheet" href="src/styles/partials/space.css">
    <link rel="stylesheet" href="src/styles/partials/meta.css">
    <link rel="stylesheet" href="src/styles/partials/image_box.css">
    <link rel="stylesheet" href="src/styles/partials/media_box.css">
    <link rel="stylesheet" href="src/styles/partials/social.css">
    <link rel="stylesheet" href="src/styles/partials/apps.css">

    {{> blocks }}
  </head>
  <body>
    {{#> layout}}
      <div data-ko-container="main" data-ko-wrap="false">
        {{> header }}
        {{> header_business }}
        {{> separator_wide }}
        {{> separator }}
        {{> space_large }}
        {{> space }}
        {{> space_small }}
        {{> image_cover }}
        {{> image_cover_small }}
        {{> h1 }}
        {{> h2 }}
        {{> h3 }}
        {{> body }}
        {{> quote }}
        {{> meta }}
        {{> button }}
        {{> button_link }}
        {{> image_box }}
        {{> image_box_2 }}
        {{> image_box_3 }}
        {{> media_box }}
        {{> media_box_alternative }}
        {{> social }}
        {{> apps }}
      </div>
    {{/layout}}
  </body>
</html>
